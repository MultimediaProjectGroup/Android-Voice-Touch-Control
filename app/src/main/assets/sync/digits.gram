#JSGF V1.0;

grammar digits;

<digit> = plus   |
          reset |
          minus |
          multiple |
          divide by |
          equal |
          zero  |
          one   |
          two   |
          three |
          four  |
          five  |
          six   |
          seven |
          eight |
          nine  |
          point |
          close calculator;

public <digits> = <digit>+;
